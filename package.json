{
  "name": "middle.messenger.praktikum.yandex",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "tsc --noEmit && vite --open",
    "start": "npm run build && node ./server.cjs",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "mocha --require ts-node/register --require tsconfig-paths/register --require ./mochaSetup.cjs './src/**/*.spec.ts'",
    "lint:ts": "eslint \"**/*.{ts, tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts, tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "lint": "npx eslint \"**/*.{ts, tsx}\" && npx stylelint \"**/*.scss\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/jsdom": "^21.1.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.6.4",
    "@types/sinon": "^17.0.2",
    "@types/sinon-chai": "^3.2.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.3.10",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "mocha": "^10.2.0",
    "node-sass": "^9.0.0",
    "sass": "^1.64.1",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-handlebars": "^1.6.0"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">12.0.0"
  }
}
